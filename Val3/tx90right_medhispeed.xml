<?xml version="1.0" encoding="UTF-8"?>
<!-- Connect TX90-right with the model with a ball on hand -->
<val3setup>
  <robot name="TX90L"/>

  <!-- Motion limits override -->
  <!-- Can also specify the rate="x" flag, where x is either a fraction "0.1" or a percent "10%"-->
  <motion qmin="-90 -75 -45 -90 -90 -270" qmax="90 60 145 90 90 270" vmax="200 200 200 200 200 200" amax="200 200 200 200 200 200"/> 

  <model name="Right arm" file="data/tx90ball.rob" />
  <map model="0" value="0.0"/>
  <map robot="0" model="1" scale="0.01745329251994329576923690768489" offset="0.0"/>
  <map robot="1" model="2" scale="0.01745329251994329576923690768489" offset="0.0"/>
  <map robot="2" model="3" scale="-0.01745329251994329576923690768489" offset="0.0"/>
  <map robot="3" model="4" scale="0.01745329251994329576923690768489" offset="0.0"/>
  <map robot="4" model="5" scale="-0.01745329251994329576923690768489" offset="0.0"/>
  <map robot="5" model="6" scale="0.01745329251994329576923690768489" offset="0.0"/>

</val3setup>